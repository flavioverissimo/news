<%- include("../partials/header.ejs")%>
<div class="form-signin w-100 m-auto py-5">
  <form class="w-25 mx-auto" method="post">
    <h1 class="h3 mb-3 fw-normal">Please log in</h1>
    
    <div class="form-floating">
      <input type="text" class="form-control" id="username" name="username" placeholder="username">
      <label for="username">username</label>
    </div>
    <div class="form-floating">
      <input type="password" class="form-control" id="password" name="password" placeholder="Password">
      <label for="password">Password</label>
    </div>
    <button class="w-100 btn btn-lg btn-primary mt-3" type="submit">Login</button>
    <hr>
    <a class="w-100 btn btn-lg btn-primary mt-3 w-25" href="/facebook">Entrar com Facebook</a>
    <a class="w-100 btn btn-lg btn-primary mt-1 w-25" href="/google">Entrar com Gmail</a>
  </form>


</div>
<%- include("../partials/footer.ejs")%>